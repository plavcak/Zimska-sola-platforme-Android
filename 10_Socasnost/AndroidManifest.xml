<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="si.feri.delavnica.demo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name="si.feri.delavnica.demo.Stevec" android:enabled="true" />
        
        <service android:name="si.feri.delavnica.demo.StevecDoSto" android:enabled="true" />
        
        <service android:name="si.feri.delavnica.demo.Delavec" android:enabled="true" />
        
    </application>

</manifest>